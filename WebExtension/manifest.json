{
  "manifest_version": 2,
  "name": "Replay",
  "version": "2.0",
  "description": "An auto replay for Youtube with custom timer.",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*.youtube.com/*" ],
      "js": [ "htube.js", "interface.js" ]
    }
  ],
  "applications": {
    "gecko": {
      "id": "htube@blankspace.shakeitoff"
    }
  }
}